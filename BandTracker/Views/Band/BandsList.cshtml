<!DOCTYPE html>
<html>
  <head>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css'>
    <link rel="stylesheet" type="text/css" href="~/css/styles.css" />
    <meta charset="utf-8">
    <title>Band Tracker</title>
  </head>
  <body>
      <div class="container">
       <h1>Band Tracker</h1>
       <nav>
        <a href="/home">Home Page</a> |
        <a href="/new/band">Add Band</a> |
        <a href="/venues/list">All Venues</a> |
        <a href="/new/venue">Add Venue</a>
      </nav>
      <h3>All current bands: </h3>
      @foreach (var band in @Model)
      {
        <ul>
          <li><a href="/band/@band.GetId()/details">@band.GetName()</li>
        </ul>
      }
    </div>
  </body>
</html>
